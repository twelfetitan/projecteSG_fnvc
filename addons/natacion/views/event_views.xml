<odoo>
    <data>
        <record model="ir.ui.view" id="natacion.event_list">
            <field name="name">natacion list</field>
            <field name="model">natacion.event</field>
            <field name="arch" type="xml">
                <list>
                    <field name="name" />
                    <field name="style_id" />
                    <field name="category_id" />
                </list>
            </field>
        </record>

        <record model="ir.ui.view" id="natacion.event_form_view">
            <field name="name">natacion.event.form</field>
            <field name="model">natacion.event</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group string="Event Data">
                            <field name="name" />
                            <field name="style_id" />
                            <field name="category_id"/>
                            <field name="session_id"/>
                            <field name="swimmer_ids"/>
                            <field name="series_ids"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
