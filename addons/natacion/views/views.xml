    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="natacion.action_club_window">
      <field name="name">natacion Club window</field>
      <field name="res_model">natacion.club</field>
      <field name="view_mode">list,form</field>
    </record>

    <record model="ir.actions.act_window" id="natacion.action_category_window">
      <field name="name">natacion Category window</field>
      <field name="res_model">natacion.category</field>
      <field name="view_mode">list,form</field>
    </record>

    <record model="ir.actions.act_window" id="natacion.action_swimmer_window">
      <field name="name">natacion Swimmer window</field>
      <field name="res_model">res.partner</field>
      <field name="view_mode">list,form</field>
    </record>

    <record model="ir.actions.act_window" id="natacion.action_besttime_window">
      <field name="name">natacion Besttime window</field>
      <field name="res_model">natacion.besttime</field>
      <field name="view_mode">list,form</field>
    </record>

    <record model="ir.actions.act_window" id="natacion.action_style_window">
      <field name="name">natacion Style window</field>
      <field name="res_model">natacion.style</field>
      <field name="view_mode">list,form</field>
    </record>


    <record model="ir.actions.act_window" id="natacion.action_championship_window">
      <field name="name">natacion Championship window</field>
      <field name="res_model">natacion.championship</field>
      <field name="view_mode">list,form</field>
    </record>

    <record model="ir.actions.act_window" id="natacion.action_session_window">
      <field name="name">natacion Session window</field>
      <field name="res_model">natacion.session</field>
      <field name="view_mode">list,form</field>
    </record>

    <record model="ir.actions.act_window" id="natacion.action_event_window">
      <field name="name">natacion Event window</field>
      <field name="res_model">natacion.event</field>
      <field name="view_mode">list,form</field>
    </record>

    <record model="ir.actions.act_window" id="natacion.action_series_window">
      <field name="name">natacion Series window</field>
      <field name="res_model">natacion.series</field>
      <field name="view_mode">list,form</field>
    </record>

    <record model="ir.actions.act_window" id="natacion.action_result_window">
      <field name="name">natacion Result window</field>
      <field name="res_model">natacion.result</field>
      <field name="view_mode">list,form</field>
    </record>

