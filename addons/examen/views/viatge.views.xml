<odoo>
    <data>
        <record model="ir.ui.view" id="examen.viatge_list">
            <field name="name">examen list</field>
            <field name="model">examen.viatge</field>
            <field name="arch" type="xml">
                <list string="Viatge">
                    <field name="conductor" />
                    <field name="furgoneta" />
                </list>

            </field>
        </record>



        <record model="ir.ui.view" id="examen.viatge_search_view">
            <field name="name">examen.viatge.search</field>
            <field name="model">examen.viatge</field>
            <field name="arch" type="xml">
                <search>
                    <field name="furgoneta" />
                    <filter string="Furgoneta" name="furgoneta_filter" domain="[('furgoneta', '!=', False)]" />
                </search>
            </field>
        </record>



    </data>
</odoo>